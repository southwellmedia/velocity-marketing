{
  "name": "velocity-main",
  "version": "0.1.0",
  "description": "Southwell Astro Boilerplate - A production-ready starter template built on Astro 6 and Tailwind CSS v4",
  "type": "module",
  "author": "Southwell Media",
  "license": "MIT",
  "keywords": [
    "astro",
    "astro-theme",
    "theme",
    "tailwind",
    "tailwindcss",
    "typescript",
    "blog",
    "starter",
    "boilerplate",
    "seo"
  ],
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "pnpm lint && pnpm check && pnpm build",
    "test": "vitest",
    "test:e2e": "playwright test",
    "showcase:capture": "npx tsx scripts/capture-showcase.ts"
  },
  "dependencies": {
    "@astrojs/mdx": "5.0.0-beta.7",
    "@astrojs/react": "5.0.0-beta.3",
    "@astrojs/sitemap": "^3.7.0",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "@fontsource-variable/manrope": "^5.2.8",
    "@fontsource-variable/outfit": "^5.2.8",
    "astro": "6.0.0-beta.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.34.0",
    "lucide-react": "^0.563.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "satori": "^0.19.1",
    "satori-html": "^0.3.2",
    "schema-dts": "^1.1.2",
    "sharp": "^0.34.5",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.7-beta.1",
    "@eslint/js": "^9.17.0",
    "@pagefind/default-ui": "^1.3.0",
    "@playwright/test": "^1.49.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.17.0",
    "eslint-plugin-astro": "^1.3.0",
    "globals": "^15.14.0",
    "pagefind": "^1.3.0",
    "prettier": "^3.4.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0",
    "vitest": "^2.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
